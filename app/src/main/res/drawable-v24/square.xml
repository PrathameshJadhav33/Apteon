<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:height="10dp" android:width="10dp" android:top="3dp" android:right="2.5dp"
        android:left="2.68dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="@color/white" />
                <stroke
                    android:width="1dp"
                    android:color="@color/orange" />
            </shape>
        </rotate>
    </item>
</layer-list>